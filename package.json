{
  "name": "www.updater.cc",
  "version": "1.0.0",
  "private": true,
  "hexo": {
    "version": "3.8.0"
  },
  "scripts": {
    "clean": "hexo clean",
    "start": "hexo clean && hexo server --log --port 4000",
    "debug": "hexo clean && hexo server --log --port 4000 --debug",
    "build": "hexo clean && hexo generate",
    "serve": "./node_modules/.bin/http-server ./public",
    "deploy": "hexo clean && hexo generate && rm public/css/*.map && hexo deploy"
  },
  "dependencies": {
    "hexo": "^3.8.0",
    "hexo-all-minifier": "0.5.2",
    "hexo-auto-canonical": "^0.1.0",
    "hexo-autolinker": "^1.0.1",
    "hexo-autonofollow": "^1.0.0",
    "hexo-browsersync": "^0.3.0",
    "hexo-deployer-s3": "^0.2.0",
    "hexo-filter-author-from-git": "^0.1.0",
    "hexo-filter-auto-spacing": "^0.2.1",
    "hexo-filter-date-from-git": "^0.1.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-sitemap": "^1.1.2",
    "hexo-generator-tag": "^0.2.0",
    "hexo-github-card": "^1.0.0",
    "hexo-migrator-rss": "^0.1.2",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-jade": "^0.4.1",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.3.3",
    "http-server": "^14.1.1"
  }
}